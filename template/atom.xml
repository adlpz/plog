<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	
	<title>%(blog_name)s</title>
    <link href="{{base_url}}/atom" rel="self" />
    <link href="{{base_url}}" />
    <id>{{base_url}}/atom</id>
    <updated>{{updated}}</updated>
	<author>
        <name>{{author}}</name>
        <email>{{email}}</email>
	</author>
	
    {{#posts}}
    <entry>
        <title>{{title}}</title>
        <link href="{{url}}" />
        <id>{{url}}</id>
        <updated>{{updated}}</updated>
        <summary>{{content}}</summary>
    </entry>
    {{/posts}}
	
</feed>
